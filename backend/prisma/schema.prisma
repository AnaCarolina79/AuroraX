generator client {
  provider = "prisma-client-js"
  previewFeatures = ["mongodb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Usuarios {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  titulo  String
  autor   String
  ativo   Boolean @default(true)
}
